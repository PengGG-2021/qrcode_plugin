{
  "manifest_version": 3,
  "name": "网页二维码生成器",
  "version": "1.0",
  "description": "在网页右下角显示当前网站的二维码",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://api.qrserver.com/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}